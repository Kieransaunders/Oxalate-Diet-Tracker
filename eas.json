{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "autoIncrement": true
    },
    "app-store-review": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_STORE_REVIEW": "true"
      }
    },
    "testflight": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_IS_TESTFLIGHT": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}